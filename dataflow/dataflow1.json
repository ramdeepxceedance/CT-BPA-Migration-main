{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Json1",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "flatten1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          eventLogDTO as (trackingId as string, businessKey as string, parentEntityId as string, parentEntityType as string, entityId as string, entityType as string, event as string, eventDesc as string, status as string, actionBy as string, payload as string, notificationDTO as string[], tenantId as string),",
				"          topic as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     documentForm: 'arrayOfDocuments',",
				"     unquotedColumnNames: true) ~> source1",
				"source1 foldDown(unroll(),",
				"     mapColumn(",
				"          eventLogDTO,",
				"          topic",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> flatten1"
			]
		}
	}
}